{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@Components/*": ["./src/Components/*"],
      "@Routes/*": ["./src/Routes/*"],
      "@Screens/*": ["src/Screens/*"],
      "@Storage/*": ["./src/storage/*"],
