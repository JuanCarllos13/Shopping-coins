{
  "expo": {
    "name": "shopping-coins",
    "slug": "shopping-coins",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7B22D3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.juancarllos13.shopping-coins",
